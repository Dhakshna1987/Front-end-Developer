
//Remove Array Duplicate

const numbers = [1, 2, 3, 4, 5, 6, 6, 8, 7, 7];


//Method1
const unique = [...new Set(numbers)];

//Method2

const unique2 = numbers.filter((item, index) => numbers.indexOf(item) === index );

//Method3

const unique3 = numbers.reduce((partial, item) => (partial.includes(item) ? partial : [...partial, item]), [] )






